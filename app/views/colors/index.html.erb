<h1>Colors#index</h1>

<table>
<thead>
    <tr>
     <th>ID</th>
     <th>hexcolor</th>
     <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
	<% @colors.each do |color|%>
	 <tr>
        <td><%= link_to color.id, color %></td>
        <td><%= link_to color.hexcolor, color %></td>
        <td><%= link_to 'Show', color %></td>
        <td><%= link_to 'Edit', edit_color_path(color) %></td>
        <td><%= link_to 'Destroy', color, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
	<%end%>
</table>

</tbody>
</table>

<br>

<%= link_to 'New Color', new_color_path %>