<!DOCTYPE html>
<html>
<head>
  <title>Testapp1</title>
  <link href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/css/bootstrap.min.css" rel="stylesheet">
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <script src="//code.jquery.com/jquery-1.11.3.min.js"></script>  
  <script src="//maxcdn.bootstrapcdn.com/bootstrap/3.3.5/js/bootstrap.min.js"></script>	
  <%= javascript_include_tag 'default', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
   <!--JQuery Media Script-->
    <script type='text/javascript' src='js/jquery.media.js'></script>
    <script type='text/javascript' src='js/jquery.stellar.min.js'></script>

</head>
<body data-spy="scroll" data-target=".navbar">

<!--h1>Welcome to bike København</h1-->
<!--h2>Your source for used bikes in København</h2-->
<!--Navigator bar stars-->
<!--header data-role="header"-->
<nav class = "navbar navbar-inverse navbar-fixed-top">
      <div class = "container-fluid">
        <div class="navbar-header">
          <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">

            <span class="sr-only">Toggle navigation</span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </button>
        </div>  
       <div class = "navbar-collapse collapse">

           <div class="projectName">
            <!--a href="#"><img src="img/logo.png" alt="logo" height="120" width="120"/></a-->
            <p>CyklershopApp</p>
          </div>
          
           <nav class = "mainmenu">
           <ul class = "nav navbar-nav">
            <!--li><a href="#contact">Contacts</a></li-->        
            <li><%= link_to 'Contacts', static_pages_contact_path %></li>
            <!--li><a href="#about">About</a></li-->
            <li><%= link_to 'About', static_pages_about_path %></li>
            <!--li><a href="#products">Products</a></li-->
            <li><%= link_to 'Products', products_path %></li>
            <!--li><a href="#">Home</a></li-->
            <li><%= link_to 'Home', root_path %></li>
          </ul>  
          <div class="search-form">
            <%= form_tag("/products", method: "get") do %>
            <%= label_tag(:q, "Search for:") %>
            <%= text_field_tag(:q) %>
            <%= submit_tag("Search") %>
            <% end %>
        </div>
          </nav>
        </div>
      </div> 
    </div> 
</nav>    
<!--/header-->
<!--Navigator bar ends-->

<%= yield %>

</body>
</html>
